<script is:inline>
  partytown = {
    forward: ['dataLayer.push'],
  }
</script>

<script type="text/partytown" src=`https://www.googletagmanager.com/gtag/js?id=${import.meta.env.PUBLIC_GA}`></script>

<script type='text/partytown'>
  if (import.meta.env.PROD) {
    function gtag() {
      window?.dataLayer?.push(arguments);
    }

    gtag('js', new Date());
    gtag('config', import.meta.env.PUBLIC_GA);
  }
</script>
